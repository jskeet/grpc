{
  "buildOptions": {
  "define": [ "SIGNED" ],
  "keyFile": "../keys/Grpc.snk",
  "publicSign": true,
  "compile": {
    "includeFiles": [ "../Grpc.Core/Version.cs" ]
  },
  },

  "dependencies": {
    "Grpc.Auth": {
      "target": "project"
    },
    "Grpc.Core": {
      "target": "project"
    },
    "Google.Protobuf": "3.0.0-beta3",
    "CommandLineParser": "1.9.71",
    "NUnit": "3.4.1",
    "dotnet-test-nunit": "3.4.0-beta-1"
  },
  "testRunner": "nunit",
  "frameworks": {
    "net451": {
      "dependencies": {
        "Moq": "4.2.1510.2205"
      },
      "frameworkAssemblies": {
        "System.Runtime": "",
        "System.IO": ""
      }
    },
    "netcoreapp1.0": {
      "imports": [
        "portable-net45"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        },
        "System.Linq.Expressions": "4.1.0"
      }
    }
  }
}
